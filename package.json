{
  "name": "a_mousy_food_fight",
  "version": "0.0.1",
  "description": "Video Game Created for Ludum Dare 32. The Theme is Unconventional Weapon",
  "main": "index.js",
  "private": true,
  "dependencies": {
    "splatjs": "^3.0.0"
  },
  "devDependencies": {
    "browserify": "^4.1.8",
    "jshint": "^2.5.1"
  },
  "scripts": {
    "lint-js": "bash -c \"jshint game.js\"",
    "build-js": "browserify game.js -o index.js",
    "build": "npm run lint-js && npm run build-js"
  },
  "author": "Anthony Quisenberry <anthony.quisenberry01@gmail.com> (http://anthonyquisenberry.com),Clay Morton <lyonsclay@yahoo.com>,Kira Winters <klwint02@gmail.com>",
  "license": "MIT",
  "jshintConfig": {
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "immed": true,
    "indent": 4,
    "latedef": "nofunc",
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonbsp": true,
    "nonew": true,
    "quotmark": "double",
    "unused": "strict",
    "trailing": true,
    "browser": true,
    "devel": true,
    "globalstrict": true,
    "globals": {
      "module": false,
      "require": false
    }
  }
}
